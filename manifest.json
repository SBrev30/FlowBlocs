{
  "manifest_version": 3,
  "name": "FlowBlocs",
  "version": "1.0.0",
  "description": "Transform Notion databases into infinite visual canvases",
  "permissions": [
    "identity",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://api.notion.com/*"
  ],
  "action": {
    "default_title": "Open FlowBlocs Canvas"
  },
     "chrome_url_overrides": {
     "newtab": "index.html"
   }
  "background": {
    "service_worker": "background.js"
  }
}
